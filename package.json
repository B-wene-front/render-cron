{
  "name": "evtol-news-cron",
  "version": "1.0.0",
  "description": "Cron job for importing EVTOL company news (Joby, Archer, Beta, etc.)",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "dev:joby": "SERVICE_NAME=joby ts-node src/index.ts",
    "dev:archer": "SERVICE_NAME=archer ts-node src/index.ts",
    "dev:beta": "SERVICE_NAME=beta ts-node src/index.ts",
    "dev:all": "SERVICE_NAME=all ts-node src/index.ts"
  },
  "keywords": [
    "cron",
    "evtol",
    "joby-aviation",
    "archer-aviation",
    "beta-technologies",
    "puppeteer",
    "web-scraping"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "puppeteer": "^21.6.0",
    "node-html-parser": "^6.1.11",
    "html-to-text": "^9.0.5",
    "voyageai": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}

